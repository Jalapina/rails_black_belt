<a href="/">Home</a>
<h1><%= @song.title %> - <%= @song.artist %></h1>
<table>

    <tr>
        <th>Name</th>
        <th>Times Added</th>
    </tr>

    <% @users.where(:song_id => @song.id).each do |user| %>
        <tr>
            <td><a href="/users/<%= user.user.id %>/show"><%= user.user.first_name %> <%= user.user.last_name %></a></td>
            <td><%= rand(1..10) %></td>
        </tr>
    <% end %>

</table>